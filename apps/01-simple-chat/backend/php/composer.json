{
    "name": "rag-demo/php-backend",
    "description": "PHP backend for RAG demo application",
    "type": "project",
    "require": {
        "php": "^8.2",
        "slim/slim": "^4.12",
        "slim/psr7": "^1.6",
        "php-di/php-di": "^7.0",
        "vlucas/phpdotenv": "^5.6",
        "monolog/monolog": "^3.5",
        "guzzlehttp/guzzle": "^7.8",
        "openai-php/client": "^0.8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable"
} 